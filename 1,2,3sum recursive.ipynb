    "def findNextN(sum, goal):\n",
    "    if sum > goal:\n",
    "        return 0\n",
    "    if sum == goal:\n",
    "        return 1\n",
    "    now = 0\n",
    "    for i in range(1, 4):\n",
    "        now += findNextN(sum+i, goal)\n",
    "    \n",
    "    return now\n",
    "\n",
    "n=int(input())\n",
    "for i in range(n):\n",
    "    print(findNextN(0, int(input())))\n"
   ]
